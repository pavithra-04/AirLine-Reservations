@{
    ViewBag.Title = "Home Page";
}
<head>
    @Styles.Render("~/Content/Index.css")
</head>
<style>
    .alertadv-v2 {
        color: #155724;
        margin-top:30px!important;
        background-color: #d4edda;
        border-color: #c3e6cb;
        padding: 13px 15px;
        border-radius: 4px;
        width: 100%;
        position: relative;
        margin: 3px auto 0 auto;
        display: table;
        align-items:center;
    }
</style>
<body>
    <!-- HERO SECTION -->
    <main>
        <section class="section-hero">
            <div class="hero">
                <div class="hero-text-box">
                    <h1 class="heading-primary">
                        FlyFriendly, the airline that makes travel easy.
                    </h1>

                    <p class="hero-description">
                        Say goodbye to the ground and hello to the skies. FlyFriendly is here to take you on an adventure like no other.
                        So why wait? Book your next trip with us today and let's soar to new heights together.
                    </p>

                    <a href="#" class="btn btn-full margin-right-sm">Fly Friendly, Fly High</a>
                    <a href="#" class="btn btn-outline">Book Now </a>

                    <div class="delivered-meals">
                        <div class="delivered-imgs">
                            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTWw_pNUAZyl9Yy2jtAdo-LPpLUjkByyawIIw&usqp=CAU" alt="customer photo" />
                            <img src="https://images.unsplash.com/photo-1490430657723-4d607c1503fc?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxzZWFyY2h8MTB8fGZsaWdodHN8ZW58MHx8MHx8&auto=format&fit=crop&w=2000&q=60" alt="customer photo" />
                            <img src="https://media.istockphoto.com/id/1434765368/photo/passenger-airplane-taking-off.jpg?b=1&s=170667a&w=0&k=20&c=Bdb2YVdK9vFzTYL8c1h5rnesMhsQJswnqV7FC7Nvu8c=" alt="customer photo" />
                            <img src="https://media.istockphoto.com/id/1463506735/photo/commercial-plane-flying-preparing-to-take-off-in-amazing-sunset.jpg?s=612x612&w=0&k=20&c=1zCQslnp_30hAyhhHx0VCQHjnyVei9JHYHe86ftwhwc=" alt="customer photo" />
                            <img src="https://media.istockphoto.com/id/1401027384/photo/businesswoman-reading-documents-and-working-on-digital-tablet-during-flight.jpg?b=1&s=170667a&w=0&k=20&c=O04HuXPRg4eszBExc4KpMe_7YCggemZsXxf4aISkQDM=" alt="customer photo" />
                            <img src="https://media.istockphoto.com/id/1386281092/photo/rear-view-asian-chinese-couple-checking-boarding-time-in-airport-waiting-in-front-arrival.jpg?b=1&s=170667a&w=0&k=20&c=bJ9S8wowgYs2vMdm6pnBFMyiUHHg2FvNSAQbqoQRyMo=" alt="customer photo" />
                        </div>
                        <p class="delivered-text">Plan Your Trip and Travel With Confidence</p>
                    </div>
                </div>

                <div class="hero-img-box">
                    <img src="https://media.istockphoto.com/id/1386281084/photo/rear-view-asian-chinese-woman-checking-boarding-time-in-airport-waiting-in-front-arrival.jpg?s=612x612&w=0&k=20&c=SnKEzNHJ5JUOL0Nh9JPFV3TUTvvJclXVVQr5CZ6Tmgg=" class="hero-img" alt="women enjoying food" />
                </div>
            </div>
        </section>

        <!-- FEATURED SECTION -->
        <section class="section-featured">
            <div class="container">
                <h2 class="heading-featured-in">Our Travel Pertners </h2>
                <div class="logos">
                    <img src="data:image/jpeg;base64," alt="techcrunch logo" />

                    <img src="data:image/jpeg;base64," alt="happy easy go" />

                    <img src="data:image/png;base64," alt="the new york times logo" />

                    <img src="data:image/png;base64," alt="forbes logo" />

                    <img src="data:image/png;base64," alt="USA today logo" />
                </div>
            </div>

        </section>

        <!-- TESTIMONIAL.... -->

        <section class="section-testimonials ">

            <div class="testimonials-container">
                <span class="subheading">Testimonials</span>
                <h2 class="heading-secondary">
                    Once you try it you can't go back
                </h2>
                <div class="testimonials">
                    <figure class="testimonial">
                        <img class="testimonial-imge" src="https://images.unsplash.com/photo-1438761681033-6461ffad8d80?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxzZWFyY2h8M3x8aHVtYW4lMjBhdmF0YXJzfGVufDB8fDB8fA%3D%3D&auto=format&fit=crop&w=2000&q=60" alt="photo" />
                        <blockquote class="testimonial-text">
                            Soaring high above the clouds, chasing the sun and living life with no regres.
                            Life is short, book a flight and see the world.
                        </blockquote>
                        <p class="testimonial-name">&mdash; Ben Hadley </p>
                    </figure>

                    <!-- 02 -->
                    <figure class="testimonial">
                        <img class="testimonial-imge" src="https://images.unsplash.com/photo-1601455763557-db1bea8a9a5a?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxzZWFyY2h8MTV8fGh1bWFuJTIwYXZhdGFyc3xlbnwwfHwwfHw%3D&auto=format&fit=crop&w=2000&q=60" alt="photo" />
                        <blockquote class="testimonial-text">
                            Exploring new destinations and creating unforgettable memories one flight at a time.
                            Adventure awaits, pack your bags and join me on this journey.
                        </blockquote>
                        <p class="testimonial-name">&mdash; Dave Bryson</p>
                    </figure>

                    <!-- 03 -->
                    <figure class="testimonial">
                        <img class="testimonial-imge" src="https://images.unsplash.com/photo-1645378999013-95abebf5f3c1?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxzZWFyY2h8MzZ8fGh1bWFuJTIwYXZhdGFyc3xlbnwwfHwwfHw%3D&auto=format&fit=crop&w=2000&q=60" alt="photo" />
                        <blockquote class="testimonial-text">
                            From city to city, country to country, taking flight and experiencing the world.
                            Up, up and away, chasing the skies and living life to the fullest.
                        </blockquote>
                        <p class="testimonial-name">&mdash; Steve Miller</p>
                    </figure>

                    <!-- 04 -->

                    <figure class="testimonial">
                        <img class="testimonial-imge" src="https://images.unsplash.com/photo-1568602471122-7832951cc4c5?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxzZWFyY2h8M3x8bWFufGVufDB8fDB8fA%3D%3D&auto=format&fit=crop&w=2000&q=60" alt="photo" />
                        <blockquote class="testimonial-text">

                            Flying to new horizons and discovering the beauty of the world.
                            Reaching new heights, one flight at a time.
                        </blockquote>
                        <p class="testimonial-name">&mdash; Hannah Smith</p>
                    </figure>
                </div>
            </div>

            <div class="gallery">
                <figure class="gallery-item">
                    <img src="https://images.unsplash.com/photo-1592985684811-6c0f98adb014?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxzZWFyY2h8MjB8fGZsaWdodHN8ZW58MHx8MHx8&auto=format&fit=crop&w=2000&q=60" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/1028421742/photo/happy-friends-traveling-by-plane-and-taking-a-selfie.jpg?s=612x612&w=0&k=20&c=wrqZBSKN3ewvUc81texwpGYIBImdTpZA1GLna_75DLE=" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://images.unsplash.com/photo-1490430657723-4d607c1503fc?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxzZWFyY2h8MTB8fGZsaWdodHN8ZW58MHx8MHx8&auto=format&fit=crop&w=2000&q=60" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/1401027269/photo/young-couple-recording-video-for-social-media.jpg?s=612x612&w=0&k=20&c=RFfePFq-uv0IDSU5xjlSZre4bHbkqJiDhbY2UXXQ33U=" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/1096223712/photo/flight-attendant-helping-a-woman-on-the-airplane.jpg?s=612x612&w=0&k=20&c=vvvPKcExh3tzZ2rec5lbAmHZWxIoIENlfLobe9oh2LU=" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/1401027626/photo/little-girl-seatbelts-teddy-into-empty-seat-next-to-her.jpg?s=612x612&w=0&k=20&c=hDyX3RqEk3uunZyHJrsY8Sd_ubxNlx5XpUXwvFEnqvU=" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/1427864085/photo/joyful-asian-woman-sits-in-the-airplane-and-listen-music-while-go-to-travel.jpg?s=612x612&w=0&k=20&c=2nACzKe_VIR8NuAM-cyc3Nl7uyhBUzbghhP-z1nTVm4=" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/1210060802/photo/business-people-traveling-by-corporate-jet.jpg?s=612x612&w=0&k=20&c=kdP7XToeDB4SvfzEjaiJeD22E5-rwxm6xTYidO5IkjI=" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/1364780340/photo/stewardess-demonstrating-how-to-use-life-vest-in-airplane.jpg?s=612x612&w=0&k=20&c=ec-w8j-RhCtNTWiveJQQFPetSCrQdg14EuJthMpMZ88=" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/1279860023/photo/passenger-wearing-face-mask-in-airplane-travel-during-covid-19.jpg?s=612x612&w=0&k=20&c=qVlklm0GfetrRQF4Rgc_hJ8VLeTzpm5USzgDXBAjmTQ=" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/1134842379/photo/woman-using-laptop-in-airplane-during-flight.jpg?s=612x612&w=0&k=20&c=MLgAU7WzzQ9cmzIMnf5vkuev-EOqshHTZFV0wgGc5UU=" alt="photo of food" />
                </figure>

                <figure class="gallery-item">
                    <img src="https://media.istockphoto.com/id/174647815/photo/vacation-photographs.jpg?s=612x612&w=0&k=20&c=nN_3svSIQLjVnCz1HeJS4n2MGW1wWUarod7pNqanI7Q=" alt="photo of food" />
                </figure>
            </div>
        </section>
           @* Alert *@
    <div class="container-fluid">
        <div class="alertadv-v2">
            <div class="advHd" style="font-size:17px;text-align:center;">
                <strong>FREE Full Refund due to Medical Reasons:</strong> FlyFriendly brings an extraordinary offer in these tough times!<br>
                Get a full refund on a domestic ticket in case you cancel it due to medical sickness. The Best Part - There are ZERO extra charges for this service.  <!--a href="https://www.easemytrip.com/pdf/Domestic-Flight-CancellationInsurance.pdf" target="_blank" style="color: #2196f3;">(View Coverage)</a -->
            </div>
        </div>
    </div>

        <!-- CALL-TO-ACTION -->

        <section class="section-cta">
            <div class="container">
                <div class="cta">
                    <div class="cta-text-box">
                        <h2 class="heading-secondary">
                            Register and Get Discount on Booking First Flight with Us
                        </h2>
                        <p class="cta-text">
                            And now, with our special offer, it's even more affordable to book your next flight with us.
                            So why wait? Take advantage of this discount today and start planning your next adventure!
                        </p>

                        <form class="cta-form" action="#">

                            <div>
                                <label for="full-name">Full Name</label>
                                <input type="text" id="full-name" placeholder="John smith" required>
                            </div>

                            <div>
                                <label for="email">Email address</label>
                                <input type="email" id="email" placeholder="me@example.com" required>
                            </div>

                            <div>
                                <label for="select-where">
                                    Where did you here from us?
                                </label>
                                <select id="select-where" required>
                                    <option value="">Please choose one option:</option>
                                    <option value="friends">Friends and family</option>
                                    <option value="Youtube">Youtube</option>
                                    <option value="podcast">Podcast</option>
                                    <option value="facebook">Facebook</option>
                                    <option value="others">Others</option>

                                </select>
                            </div>

                            <button class="btn btn--form">Sign up now</button>
                        </form>
                    </div>

                    <div class="cta-img-box" role="img" aria-label="women enjoying food">
                    </div>
                </div>
            </div>
        </section>

    </main>

    


</body>