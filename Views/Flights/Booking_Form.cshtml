/*! CSS Used from: https://www.justfly.com/css/index2.1548881590.css ; media=all */
@@media all{
.ellipsis-wrap{width:100%;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
.ellipsis-wrap{display:inline-block;vertical-align:middle;width:auto;max-width:100%;}
.form-field-button a.form-field-submit-button{display:none;}
.item-hotels-search{opacity:0;margin-top:-1px;-webkit-transition:opacity .5s 1s;-moz-transition:opacity .5s 1s;-ms-transition:opacity .5s 1s;-o-transition:opacity .5s 1s;transition:opacity .5s 1s;}
.item-hotels-search.after-recent-searches{display:none;}
.item-hotels-search.before-recent-searches{margin-bottom:3px;}
.item-hotels-search input{display:none;}
.item-hotels-search input[type=checkbox] + label{display:inline-block;position:relative;color:#FFF;font-size:14px;font-weight:600;cursor:pointer;padding-left:18px;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none;}
.item-hotels-search input[type=checkbox] + label span{display:inline-block;vertical-align:middle;position:relative;height:14px;line-height:15px;}
.item-hotels-search input[type=checkbox] + label:before{font:normal normal normal 14px/1 FontAwesome;position:absolute;content:"\f096";top:50%;margin-top:-5px;line-height:13px;font-size:15px;left:0;}
.item-hotels-search input[type=checkbox]:checked + label:before{content:"\f046";}
@@media screen and (min-width: 876px){
.item-hotels-search{margin-top:0;}
.item-hotels-search.after-recent-searches{display:inline-block;}
.item-hotels-search.before-recent-searches{display:none;}
}
}
